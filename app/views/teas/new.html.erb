<h3>Create a Tea</h3>
<%= form_with model: @tea do |form| %>
  <%= form.text_field :name, placeholder: "name" %>
  <%= form.text_field :kind, placeholder: "kind" %>
  <%= form.text_field :desc, placeholder: "description" %>
  <%= form.submit %>
<% end %>

